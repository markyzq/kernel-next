/dts-v1/;
#include "rk3288.dtsi"

/ {
	compatible = "rockchip,rk3288-tmp";

	memory {
		reg = <0x60000000 0x80000000>;
	};

	soc {
		watchdog@ff800000 {
			status = "okay";
		};

		/* uart_bt */
		serial@ff180000 {
			status = "okay";
		};

		/* uart_bb */
		serial@ff190000 {
			status = "okay";
		};

		/* uart_dbg */
		uart2: serial@ff690000 {
			status = "okay";
		};

		/* uart_gps */
		uart3: serial@ff1b0000 {
			status = "okay";
		};

		/* uart_exp */
		uart4: serial@ff1c0000 {
			status = "okay";
		};
	};
};
